@model IEnumerable<SchoolRegistrationSystem.Models.Registration>

@{
    ViewData["Title"] = "Index";
}

<p>
    <a class="btn btn-info" asp-action="Create">Add New Student</a>
</p>
<table id="pickStudent" class="table">
    <thead>
        <tr>
            <th>
                Course Name
            </th>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Course.CourseName
                @*@Html.DisplayFor(modelItem => item.StudentId)*@
            </td>
            <td>
                @item.Student.FirstName
                @* @Html.DisplayFor(modelItem => item.CourseId)*@
            </td>
            <td>
                @item.Student.LastName
            </td>
           
        </tr>
}
    </tbody>
</table>
